EXECUTABLE   co2_containment

DEFAULT <ROOT_DIR> -1
DEFAULT <OUT_DIR> -1
DEFAULT <CONTAINMENT_POLYGON> -1
DEFAULT <HAZARDOUS_POLYGON> -1
DEFAULT <ZONEFILE> -1
DEFAULT <EGRID> -1
DEFAULT <UNRST> -1
DEFAULT <INIT> -1

ARGLIST   <CASE>  <CALC_TYPE_INPUT>   "--root_dir" <ROOT_DIR> "--out_dir" <OUT_DIR> "--containment_polygon" <CONTAINMENT_POLYGON> "--hazardous_polygon" <HAZARDOUS_POLYGON> "--zonefile" <ZONEFILE>  "--egrid"  <EGRID>  "--unrst"  <UNRST> "--init" <INIT>

MIN_ARG 2
MAX_ARG 10
 
